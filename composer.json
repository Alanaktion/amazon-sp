{
    "name": "alanaktion/amazon-sp",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api"
    ],
    "license": "proprietary",
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "autoload": {
        "psr-4": { "AmazonSP\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "AmazonSP\\" : "test/" }
    }
}
